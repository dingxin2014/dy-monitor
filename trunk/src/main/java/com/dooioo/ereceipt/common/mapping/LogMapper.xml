<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dooioo.ereceipt.common.dao.LogMapper">


	<insert id="save" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO T_ERECEIPT_LOG(operate,result,remark,operator,operateTime)
		VALUES(#{operate,jdbcType=VARCHAR},#{result,jdbcType=VARCHAR},#{remark,jdbcType=VARCHAR},#{operator,jdbcType=INTEGER},#{operateTime,jdbcType=TIMESTAMP})
	</insert>
	
	
	

	
</mapper>